const person = {
  name: "Rahim",
  age: 30,
  profession: "Engineer"
};

console.log(person.name); // "Rahim"
console.log(person["age"]); // 30

const student = {
  id: 101,
  name: "Karim",
  marks: {
    math: 80,
    physics: 75,
    chemistry: 85
  }
};

console.log(student.marks.math); // 80
console.log(student["marks"]["physics"]); // 75
